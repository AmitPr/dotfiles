!urxvt*font: -*-tamsyn-medium-r-normal-*-*-87-*-*-*-*-*-*
!urxvt*boldFont: -*-tamsyn-bold-r-normal-*-*-87-*-*-*-*-*-*
!urxvt*italicFont: -*-tamsyn-medium-r-normal-*-*-87-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-tamsyn-bold-r-normal-*-*-87-*-*-*-*-*-*

!urxvt*font: -*-terminus-medium-r-normal-*-12-*-*-*-*-*-*-*
!urxvt*boldFont: -*-terminus-bold-r-normal-*-12-*-*-*-*-*-*-*
!urxvt*italicFont: -*-terminus-medium-r-normal-*-12-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-terminus-bold-r-normal-*-12-*-*-*-*-*-*-*

!urxvt*font: xft:inconsolata:size=11
!urxvt*boldFont: xft:inconsolata:bold:size=11
!urxvt*italicFont: xft:inconsolata:italic:size=11
!urxvt*font: -*-gohufont-medium-r-normal-*-11-*-*-*-*-*-*-*
!urxvt*boldFont: -*-gohufont-medium-r-normal-*-11-*-*-*-*-*-*-*
!urxvt*italicFont: -*-gohufont-medium-r-normal-*-11-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-gohufont-medium-r-normal-*-11-*-*-*-*-*-*-*
!urxvt*font: -*-terminus-medium-r-normal-*-11-*-*-*-*-*-*-* !urxvt*boldFont: -*-terminus-medium-r-normal-*-11-*-*-*-*-*-*-* !urxvt*italicFont: -*-terminus-medium-r-normal-*-11-*-*-*-*-*-*-* !urxvt*bolditalicFont: -*-terminus-medium-r-normal-*-11-*-*-*-*-*-*-* !urxvt*font: -*-tamsyn-medium-*-*-*-12-*-*-*-*-*-*-* !urxvt*boldFont: -*-tamsyn-medium-*-*-*-12-*-*-*-*-*-*-* !urxvt*italicFont: -*-tamsyn-medium-*-*-*-12-*-*-*-*-*-*-*
!urxvt*bolditalicFont: -*-tamsyn-medium-*-*-*-12-*-*-*-*-*-*-*

Xft.dpi:	96
Xft.rgba:	rgb
Xft.antialias: 	true
Xft.hinting: 	true
Xft.autohint: 	false
Xft.hintstyle: 	hintslight
Xft.lcdfilter: 	lcddefault

xclock*analog: false
xclock*background: #000000
xclock*foreground: #FFFFFF
